generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  uid    Int    @id @default(autoincrement())
  rfid   String @unique
  uname  String
  sid    String
  vmodel String
  phone  String
}

model Parking {
  pid Int @id @default(autoincrement())
  rfid String
  status String  
}
